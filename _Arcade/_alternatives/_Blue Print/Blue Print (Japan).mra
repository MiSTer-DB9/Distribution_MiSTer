        <dip bits="4,5"   name="Bonus Life" ids="20K/100K+,40K/200K+,80K/400K+,120K/600K+"/>
        <dip bits="6,7"   name="Lives" ids="2,3,4,5"/>
        <!-- DIP Switch 2 -->
        <dip bits="8,9"   name="Coinage" ids="2c/1cr,1c/1cr,1c/2cr,1c/3cr"/>
