        <!-- Tile ROMs (2× 4KB = 8KB) -->
        <part crc="43718c34" name="bg-1j.3c" length="0x800"/>
        <part repeat="0x800">FF</part>
        <part crc="d3ce077d" name="bg-2j.3d" length="0x800"/>
        <part repeat="0x800">FF</part>
        <!-- Sprite ROMs (3× 4KB = 12KB, R/B/G bitplanes) -->
